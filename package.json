{
    "name": "rpids",
    "productName": "RPIDS",
    "version": "1.0.0",
    "main": "./electron/main.js",
    "scripts": {
        "start": "electron .",
        "pack": "electron-packager . rpids --overwrite --platform=win32 --arch=ia32 --extra-resource=./engine/executables/export_data.exe --extra-resource=./engine/executables/import_data.exe --extra-resource=./engine/executables/dash_server --icon=build/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName='RPIDS'"
    },
    "devDependencies": {
        "electron": "^11.2.3",
        "electron-packager": "^15.2.0"
    },
    "dependencies": {
        "bootstrap": "^4.6.0",
        "electron-is-dev": "^2.0.0",
        "jquery": "^3.6.0",
        "popper.js": "^1.16.1",
        "python-shell": "^2.0.3",
        "sweetalert2": "^10.16.6"
    },
    "license": "GNU"
}
